@model IEnumerable<ProdBuilder0920.Domain.PartType>


@{
    ViewBag.Title = "DeletePartType";
}
<br/>
<br/>
<br/>

<h2>Delete a PartType</h2>

<h2 class="alert-warning">All Parttypes in DB</h2>
<table class="col-md-6 table table-striped table-bordered table-hover" style="width: 500px">
    <tr>
        <th>Paart type ID</th>
        <th>Name</th>
        <th>Package</th>
        <th></th>
        
    </tr>

    @foreach (var item in Model)
    {

        <tr>
            <td>@item.PartTypeID</td>
            <td>@item.PartTypeName</td>
            <td>@item.PackageId</td>
            
           
            <td>@Html.ActionLink("Delete Parttype", "DeletePartTypeIn", "Admin", new { id = item.PartTypeID }, new { @class = "btn btn-danger delete" })</td>
        </tr>
    }
</table>

